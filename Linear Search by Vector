#include <iostream>
#include <bits/stdc++.h>
#include <math.h>
#include<vector> 
using namespace std;

void LinearSearch(vector<int> arr, int size, int element)
{
	bool flag = false;
	for(int i= 0; i<size; i++)
	{
		if(arr[i] == element)
			flag=true;
	}
	if(flag)
		cout<<" ELEMENT FOUND";
	else
		cout<<" ELEMENT NOT FOUND";
}

int main() {
  vector<int> arr = {1,2,4,6,8,34,12};
  LinearSearch(arr, 7, 6);
  
  
}
