 int main(){
 vector<int> arr ;
  arr.push_back(1);
  arr.push_back(2);
  arr.push_back(3);
  arr.push_back(4);
  for(int i =0; i<arr.size(); i++)
  {
	  cout<<arr[i]<<endl;
  }
  arr.pop_back();
  arr.pop_back();
  arr.pop_back();
  for(int i =0; i<arr.size(); i++)
  {
	  cout<<arr[i]<<endl;
  }
  }
